# Serpentine set_pixel helper
def set_pixel(row, col, color):
    if row % 2 == 0:
        idx = row * WIDTH + col
    else:
        idx = row * WIDTH + (WIDTH - 1 - col)
    if 0 <= idx < WIDTH * HEIGHT:
        np[idx] = color  

# Render one frame
def display_frame(frame_data):
    for i, color in enumerate(frame_data):
        row = i // WIDTH
        col = i % WIDTH
        set_pixel(row, col, color)
    np.write()

# Animation loop
c=0

while c<5:
    for frame in frames:
        display_frame(frame)
        time.sleep(0.2)
        c+=1# 4 FPS
np.fill((255, 0, 0))          # clear all LEDs
np.write()



