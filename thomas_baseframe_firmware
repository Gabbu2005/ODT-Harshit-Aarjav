from machine import Pin
import neopixel
import time

# NeoPixel matrix setup
WIDTH = 10
HEIGHT = 6
np = neopixel.NeoPixel(Pin(26), WIDTH * HEIGHT)
np.fill((0, 0, 0))          # clear all LEDs
np.write() 
